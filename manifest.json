{
    "manifest_version":2,
    "name":"SpeedChanger",
    "version":"0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts":[
        {
            "matches":["https://www.youtube.com/*"],
            "css":["style.css"],
            "js":["content.js"]
        }
    ],
    "permissions": [
        "tabs",
        "http://www.youtube.com/*"
    ]
}